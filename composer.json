{
    "name": "dancos/cpf-cnpj-bundle",
    "description": "Bundle para validação de CPF e CNPJ no Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dancos\\Bundle\\CpfCnpjBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dancos\\Bundle\\CpfCnpjBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-intl": "*",
        "symfony/validator": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/framework-bundle": "^6.0|^7.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "phpunit/phpunit": "^10.1"
    },
    "config": {
        "sort-packages": true
    }
}
